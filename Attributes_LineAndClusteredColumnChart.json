						"secShow": true,
						"alignZeros": true,
						"secPosition": "Right",
						"secStart": 0,
						"secEnd": 0,
						"secAxisScale": "linear",
						"secLabelColor": {"solid": {"color": "#949494"}},
						"secFontSize": 8,
						"secFontFamily": "Arial",
						"secBold": true,
						"secItalic": false,
						"secUnderline": false,
						"secLabelDisplayUnits": 0,
						"secLabelPrecision": 0,
						"secShowAxisTitle": true,
