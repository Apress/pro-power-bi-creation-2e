						"secTitleColor": {"solid": {"color": "#949494"}},
						"secTitleFontSize": 8,
						"secTitleFontFamily": "Arial",
						"secTitleBold": true,
						"secTitleItalic": false,
						"secTitleUnderline": false,
						"invertAxis": false							
					}],
			"legend": [{
						"show": true,
						"position": "Top",
						"showTitle": true,
						"titleText": "Add a legend title",
						"legendColor": {"solid": {"color": "#949494"}},
						"fontFamily": "Arial",
						"fontSize": 8,
						"bold": true,
						"italic": false,
						"underline": false,
						"legendMarkerRendering":"markerOnly"												
