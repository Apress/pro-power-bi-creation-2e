						"show": true,
						"titleFontColor":{"solid":{"color":"#ffffff"}},
						"valueFontColor":{"solid":{"color":"#FFFFFF"}},
						"fontSize": 11,
						"fontFamily": "Arial Black",
						"background":{"solid":{"color":"#D1CAB8"}},
						"transparency": 8,
						"bold": true,
						"italic": false,
						"underline": false
						}],
			"general":  [{
						"responsive": true,
						"altText": "Enter an alternative text" 
						}],
			"padding": [{
						"left":3,
						"right":3,
						"top":3,
						"bottom":3
						}]
			}
		}
	}
}